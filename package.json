{
  "name": "react-next-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'pages/**/*.{js,jsx,tsx,ts}'",
    "lint:fix": "eslint '**/*.{js,jsx,tsx,ts}' --fix",
    "lint:style": "stylelint '**/*.css'",
    "lint:style:fix": "stylelint '**/*.css' --fix",
    "tcm": "tcm pages -c -w & tcm styles -c -w"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-import": "^1.13.1",
    "gray-matter": "^4.0.2",
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "remark-html": "^13.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": "eslint '**/*.{js,jsx,tsx,ts}'",
    "*.{css}": "stylelint '**/*.css'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.14.8",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "typed-css-modules": "^0.6.4",
    "typescript": "^4.0.5"
  }
}
